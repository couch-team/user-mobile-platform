{
  "expo": {
    "name": "Couch App",
    "slug": "couch-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/couch180.png",
    "backgroundColor": "#060C23",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#060C23"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "27",
      "bundleIdentifier": "com.joincouch.app",
      "icon": "./assets/couch180.png",
      "iconImageSet": {
        "120x120": "./assets/couch120.png",
        "180x180": "./assets/couch180.png",
        "76x76": "./assets/couch76.png",
        "152x152": "./assets/couch152.png",
        "167x167": "./assets/couch167.png"
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "versionCode": 14,
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.joincouch.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/foreground.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9fcad263-2bd7-40b2-aa67-4ddebc41929d"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "Allow Couch App to access your microphone."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Couch App to access your camera",
          "microphonePermission": "Allow Couch App to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "owner": "couch-app"
  }
}
